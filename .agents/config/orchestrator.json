    "codeReview": {
      "enabled": true,
      "automaticApproval": false,
      "minimumApprovals": 1,
      "reviewerAgent": "code-review-agent"
    },
    "testing": {
      "requireTests": true,
      "minimumCoverage": 90,
      "runTestsBeforeMerge": true
