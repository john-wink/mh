{
  "orchestrator": {
    "name": "Manhunt Development Orchestrator",
    "version": "1.0.0",
    "currentSprint": 1,
    "sprintDuration": "1 week",
    "projectRoot": "/Users/johnwink/Herd/manhunt",
    "boardDirectory": "/Users/johnwink/Herd/manhunt/.board",
    "agentDirectory": "/Users/johnwink/Herd/manhunt/.agents"
  },
  "settings": {
    "maxConcurrentAgents": 28,
    "autoAssignTasks": true,
    "requireHumanApproval": {
      "architectureChanges": true,
      "breaking Changes": true,
      "securityChanges": true,
      "databaseMigrations": false
